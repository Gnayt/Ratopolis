#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/aptos-cli/bin/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/aptos-cli/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/aptos-cli/bin/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/aptos-cli/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules/@aptos-labs/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/@aptos-labs+aptos-cli@0.2.0/node_modules:/workspaces/Ratopolis/SetupCollection/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/aptos" "$@"
else
  exec node  "$basedir/../../bin/aptos" "$@"
fi
